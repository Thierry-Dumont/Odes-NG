cmake_minimum_required(VERSION 2.8)
set(CMAKE_CXX_COMPILER "icpc")
project(NewRadau5)
include_directories(
${CMAKE_SOURCE_DIR}/../include
)
add_executable(
	run
	../main.cc
)
target_link_libraries(
  run
  mkl_intel_lp64
  mkl_sequential
  mkl_core
)
set(CMAKE_CXX_FLAGS "-O3 -ansi-alias -parallel -opt-subscript-in-range -Wall  -DINTELCOMP")
#set(CMAKE_CXX_FLAGS "-O3 -ansi-alias -parallel -opt-subscript-in-range -Wall -guide -guide-file=toto -DLOGRADAU5  -DINTELCOMP")